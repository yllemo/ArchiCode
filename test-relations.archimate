#spacing: 100
#fontSize: 12
#direction: down

// Test alla ArchiMate relationstyper och syntaxer

// Serving (solid line, open arrow)
[<business:actor> A1]
[<business:service> B1]
[A1] -> [B1]
[B1] <- [A1]

// Flow (dashed line, open arrow)
[<business:process> A2]
[<business:process> B2]
[A2] --> [B2]
[B2] <-- [A2]

// Triggering (solid line, open arrow)
[<business:process> A3]
[<business:process> B3]
[A3] -|> [B3]
[B3] <|- [A3]

// Access (dashed line, open arrow, bidirectional)
[<application:component> A4]
[<application:data> B4]
[A4] <-> [B4]

// Realization (dashed line, empty triangle)
[<application:component> A5]
[<application:service> B5]
[A5] --:> [B5]
[B5] <:-- [A5]

// Specialization (solid line, empty triangle)
[<business:role> A6]
[<business:role> B6]
[A6] -:> [B6]
[B6] <:- [A6]

// Assignment (solid line, filled circle)
[<business:actor> A7]
[<business:process> B7]
[A7] .-- [B7]
[B7] --. [A7]

// Composition (solid line, filled diamond)
[<application:component> A8]
[<application:component> B8]
[A8] +- [B8]
[B8] -+ [A8]

// Aggregation (solid line, empty diamond)
[<business:service> A9]
[<business:process> B9]
[A9] o- [B9]
[B9] -o [A9]

// Association (solid line, no arrow)
[<business:object> A10]
[<business:object> B10]
[A10] --- [B10]
